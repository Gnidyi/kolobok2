#include <stdio.h>
#include <cs50.h>

int main(void)
{
    int A;
    int B;

 do
    {
        A = get_int("A: ");
    }
    while (A < 0);

    do
    {
        B = get_int("B: ");
    }
    while (A > B);

 int suma = 0;
 int i = A;

while (B >= A)
{
    suma += i;
    A++;
    i = A;
}

 printf("suma: %i \n", suma);
}
